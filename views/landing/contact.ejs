    <div id="closespace" class = "ui container" style="padding:20px;">
    <h2 class="ui center aligned icon header">
        <i class="envelope icon" style="margin: 30px"></i>
        <div class="content" style="font-size:25px;">Interested in learning more about SLIDE or want to give an announcement at our meeting?</div> 
        <div class="ui header center aligned"  style="font-size:25px;"> Feel free to contact us with any questions you may have!</div>

     </h2>  
       <p style="text-align:center;padding-bottom:0px;color:#527d9e">Fill out this form below, or contact any of our '19-'20 Executive Staff members</p>
              <div class="ui container" style = "padding: 10px;">
             <% if(success && check &&  success.length > 0){ %>   
            <div class="ui icon positive message">
              <i class="checkmark icon"></i>
              <i class="close icon"></i>
              <div class="content">
                <div class="header">
                  <%= success %>
                </div>
                <p><%=check%></p>
              </div>
            </div>
        <% } %>
        
        <% if(success && success.length > 0 && !check){ %>   
            <div class="ui icon positive message">
              <i class="checkmark icon"></i>
              <i class="close icon"></i>
              <div class="content">
                <div class="header">
                  <%= success %>
                </div>
              </div>
            </div>
        <% } %>
        
         <% if(failure && failure.length > 0){ %>   
            <div class="ui icon negative message">
              <i class="frown icon"></i>
              <i class="close icon"></i>
              <div class="content">
                <div class="header">
                  <%= failure %>
                </div>
              </div>
            </div>
        <% } %>
        </div>
   
        <div class="ui divider" style="margin-top: 50px"></div>

    
        <div class="ui centered header" style="font-size:25px;">CONTACT US!</div> 

   
        
        <div class="ui middle aligned center aligned grid">
            <div class=" nine wide column">
                 <form class="ui small form" action="/contact" method="POST">
                
                    <div class=" required field">
                    <label>Name</label>
                    <input type="text" placeholder="Your Name" name="name">
                  </div> 
                  <div class="required field">
                    <label>Email</label>
                    <input type="email" placeholder="Your Email Address" name="email">
                  </div>
                  <div class="required field">
                    <label>Subject</label>
                    <input type="text" placeholder="What do you want to tell us" name="subject">
                  </div>
                   <div class=" required field">
                <label>Text</label>
                <textarea rows="5" cols="73%" name="text"></textarea>
              </div>
              
              <div style="margin-top:20px">
                  
                  <button class="ui submit blue big  button"><i class="envelope icon" style="margin-right: 10px"></i>Submit</button>
                </div>
                
              
            </form>
            </div>
        </div>
       
           
    
    
    
    <div class="ui divider" style="margin: 50px"></div>
    

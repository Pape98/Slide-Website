    // create sidebar and attach to menu open
    $('.ui.sidebar').sidebar();
  });
</script>
<script>
  $('.message .close')
    .on('click', function () {
      $(this)
        .closest('.message')
        .transition('fade');
    });
  $(".alternate").click(function () {
    $(".login.modal").modal({
        blurring: true
      }).modal('setting', 'transition', 'Vertical Flip')
      .modal('show');
  });
  $("#loginTab").click(function () {
    $(".login.modal").modal({
        blurring: true
      }).modal('setting', 'transition', 'Vertical Flip')
      .modal('show');
  });


  $(".signup").click(function () {
    $(".choice.modal").modal({
        blurring: true
      }).modal('setting', 'transition', 'Vertical Flip')
      .modal('show');
  });

  $('#memberSignUp').click(function () {
    $(".ui.modal.member").modal({
        blurring: false
      }).modal('setting', 'transition', 'Vertical Flip')
      .modal('show');
  });

  $('#staffSignUp').click(function () {
    $(".ui.modal.staff").modal({
        blurring: false
      }).modal('setting', 'transition', 'Vertical Flip')
      .modal('show');
  });
</script>
<script type="text/javascript">
  $('#form1')
  form({
    on: 'blur',
    inline: 'true',
    fields: {
      firstName: {
        identifier: 'firstName',
        rules: [{
          type: 'empty',
          prompt: 'Please enter your first name'
        }]
      },
      lastName: {
        identifier: 'lastName',
        rules: [{
          type: 'empty',
          prompt: 'Please enter your last name'
        }]
      },
      email: {
        identifier: 'email',
        rules: [{
          type: 'empty',
          prompt: 'Please enter a valid email address'
        }]
      },
      accessCode: {
        identifier: 'accessCode',
        rules: [{
            type: 'empty',
            prompt: 'Please enter a valid access code '
          },
          {
            type: 'isExactly[379268]',
            prompt: 'Access Code is incorrect'
          }
        ]
      },
      password: {
        identifier: 'password',
        rules: [{
            type: 'empty',
            prompt: 'Please enter a password'
          },
          {
            type: 'minLength[6]',
            prompt: 'Your password must be at least {ruleValue} characters'
          }
        ]
      },
      username: {
        identifier: 'username',
        rules: [{
            type: 'empty',
            prompt: 'Please enter valid UIN'
          },
          {
            type: 'exactLength[9]',
            prompt: 'UIN has to contain 9 digits'
          }
        ]
      },
      major: {
        identifier: 'major',
        rules: [{
          type: 'empty',
          prompt: 'Please enter your major'
        }]
      },
      phoneNumber: {
        identifier: 'phoneNumber',
        rules: [{
          type: 'empty',
          prompt: 'Please enter phone number'
        }]
      },
      birthday: {
        identifier: 'birthday',
        rules: [{
          type: 'empty',
          prompt: 'Please enter your birthday'
        }]
      },
      subcommittee: {
        identifier: 'subcommittee',
        rules: [{
          type: 'empty',
          prompt: 'Please select your subco'
        }]
      }
    }
  });
</script>